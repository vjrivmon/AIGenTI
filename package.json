{
  "name": "asistente-desarrollo",
  "version": "2.0.0",
  "description": "Asistente de desarrollo profesional para análisis de código, documentación y mejores prácticas",
  "main": "dist/prueba-mcp.js",
  "types": "dist/prueba-mcp.d.ts",
  "bin": {
    "mcp": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/prueba-mcp.js",
    "dev": "ts-node src/prueba-mcp.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "desarrollo",
    "análisis",
    "código",
    "documentación",
    "mejores-prácticas",
    "typescript"
  ],
  "author": "AIGenTI",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "prisma": "^5.10.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "bcrypt": "^5.1.1",
    "chalk": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AIGenTI/asistente-desarrollo.git"
  },
  "bugs": {
    "url": "https://github.com/AIGenTI/asistente-desarrollo/issues"
  },
  "homepage": "https://github.com/AIGenTI/asistente-desarrollo#readme"
}
